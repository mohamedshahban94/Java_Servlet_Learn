<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  <servlet-name>form</servlet-name>
  <servlet-class>com.jsp.servlet.ServletDemo</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>form2</servlet-name>
  <servlet-class>com.jsp.servlet.ServletByHttpServlet</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>form3</servlet-name>
  <servlet-class>com.jsp.servlet.ServletByAnnotation</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>form</servlet-name>
  <url-pattern>/form</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  <servlet-name>form2</servlet-name>
  <url-pattern>/form2</url-pattern>
  </servlet-mapping>
 <servlet-mapping>
  <servlet-name>form3</servlet-name>
  <url-pattern>/form3</url-pattern>
  </servlet-mapping>
 
</web-app>
		
